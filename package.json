    "@types/node": "10.10.3",
    "@types/mocha": "5.0.0",
    "@types/pino": "4.7.1",
    "@types/supertest": "2.0.4",
    "chai": "4.1.2",
    "mocha": "5.0.5",
    "nyc": "11.4.1",
    "supertest": "3.0.0",
    "ts-node": "5.0.1",
    "typescript": "2.7.2"
  }
}
