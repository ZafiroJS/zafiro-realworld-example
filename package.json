  "main": "index.js",
  "scripts": {
    "setup": "sh ./bin/setup.sh",
    "start": "ts-node ./src/server.ts",
    "test": "npm run lint & npm run nyc",
    "lint": "tslint -c tslint.json ./**/*.ts",
